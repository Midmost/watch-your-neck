{
    "name": "Watch your neck!",
    "description": "You can watch your turtle neck problem while using your computer with webcam",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/V_orange.png",
        "32": "/images/V_orange.png",
        "48": "/images/V_orange.png",
        "128": "/images/V_orange.png"
      }
    },
    "icons": {
      "16": "/images/V.png",
      "32": "/images/V.png",
      "48": "/images/V.png",
      "128": "/images/V.png"
    }
    
  }